#include <bits/stdc++.h>
using namespace std;
 
int main() {
	char a[9],b[7];
	cin>>a>>b;
	int x=strlen(a),y=strlen(b);
	x++;
	y++;
	int T[x][y];
	for (int i=0;i<x;i++){
	for (int j=0;j<y;j++){
		T[i][j]=-1;
	}
	}
	for (int i=0;i<x;i++){
		T[i][0]=i;
	}
	for (int i=0;i<y;i++){
		T[0][i]=i;
	}
	int left,top,corner;
	for (int i=1;i<x;i++){
		for (int j=1;j<y;j++){
			left=T[i-1][j]+1;
			top=T[i][j-1]+1;
			corner=T[i-1][j-1]+(a[i-1]!=b[j-1]);
			T[i][j]=min(min(left,top),corner);
		}
	}
	cout<<T[x-1][y-1]<<endl;
	for (int i=0;i<x;i++){
	for (int j=0;j<y;j++){
		cout<<T[i][j]<<" ";
	}cout<<endl;}
	return 0;
}
